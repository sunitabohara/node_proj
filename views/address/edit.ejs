<% include ../header %>
<section id="maincontent" class="inner">
    <div class="container">
        <div class="col-xs-12  col-md-8">
            <% result.forEach(function(m){ %>

            <form class="form-horizontal" action="" method="post">
                <div class="form-group row">
                    <div class="col-sm-2">
                        <label>State</label>
                    </div>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="state" value="<%-m.state %> " readonly="readonly">
                    </div>

                </div>
                <div class="form-group row">
                    <div class="col-sm-2">
                        <label>District</label>
                    </div>
                    <div class="col-sm-10">
                        <input type="text" class="form-control col-sm-" name="district" value="<%-m.district %>">
                    </div>

                </div>
                <div class="form-group row">
                    <div class="col-sm-2">
                        <label>Address</label>
                    </div>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="address" value="<%-m.address %>">
                    </div>
                </div>
                <div class="row">
                    <input type="submit" class="button btn-success" name="submit">
                </div>
            </form>
            <% }) %>

        </div>
    </div>
</section> <!-- end maincontent -->

<% include ../footer %>